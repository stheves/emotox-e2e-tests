{
    "compilerOptions": {
        "target": "es5",
        "module": "ESNext",
        "lib": ["es6", "dom"],
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "sourceMap": false,
        "allowJs": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "types": ["cypress", "@testing-library/cypress", "node"]
    },
    "include": ["**/*.ts"]
}
